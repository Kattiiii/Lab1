using System;

namespace ConsoleApp8
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Введи число х : ");
            double x = Convert.ToDouble(Console.ReadLine());
            double p = (x*(x*(x*(3x-5)+2)-1)+7);
            Console.WriteLine ("Результат вычисления : " + p);
        }
    }
}
